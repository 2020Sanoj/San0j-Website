	<script type="text/javascript">
	    jQuery(function() {
		'use strict';
		var url = 'upload/';
		jQuery('#fileupload').fileupload({
		    url: url,
		    dataType: 'json',
		    done: function(e, data) {
			var is_error = data.result.files[0].error;
			if (!is_error) {
			    var uploaded_file_url = data.result.files[0].url;
			    var uploaded_file_name = data.result.files[0].name;

			    var srt = $("#upload_form").serializeArray();
			    srt.push({name: 'file', value: uploaded_file_url});
			    srt.push({name: 'file_name', value: uploaded_file_name});

			    jQuery.ajax({
				type: 'POST',
				url: 'form_values.php',
				data: srt,
				success: function(message) {
				    if (message === "ok") {
//					alert("Well done! now let's go to retrieve the package...");
					window.location.replace("done");
				    } else {
					alert("Oops! Are you shure this file is an image file? Please check again.\n\nIf you are in trouble please contact us.");
				    }
				}
			    });
			} else {
			    alert(data.result.files[0].error);
			}
		    },
		    start: function() {
			jQuery("a#tasto_upload").children("i").removeClass("fa-cloud-upload").addClass("fa-spinner fa-spin");
			$('#pt2').addClass('rotating');
		    },
		    stop: function() {
			jQuery("a#tasto_upload").children("i").removeClass("fa-spinner fa-spin").addClass("fa-cloud-upload");
			$('#pt2').removeClass('rotating');
		    },
		    progressall: function(e, data) {
			var progress = parseInt(data.loaded / data.total * 100, 10);
			$('#progress .bar').css('width', progress + '%');
		    }
		}).prop('disabled', !$.support.fileInput)
			.parent().addClass($.support.fileInput ? undefined : 'disabled');
	    });

	    jQuery('#IEtilecolor').ColorPicker({
		onSubmit: function(hsb, hex, rgb, el) {
		    jQuery(el).val(hex);
		    jQuery(el).ColorPickerHide();
		    jQuery(".colorpicker").hide();
		},
		onBeforeShow: function() {
		    jQuery(this).ColorPickerSetColor(this.value);
		}
	    });
	    jQuery("input#websiteURL").on("blur", function() {
		var URL = jQuery(this).val();
		if (URL !== "http(s)://www.yourWebsite.com/") {
		    jQuery("a#retrieveTitle").fadeIn();
		}
		checkTrailingSlash(URL, jQuery(this));
	    });
	    function checkTrailingSlash(URL, target) {
		glow = target.siblings(".warning");

		letsGlow = function() {
		    glow.hasClass('glow') ? glow.removeClass('glow') : glow.addClass('glow');
		};
		if (URL.substring(URL.length - 1) !== "/")
		{
		    glow.show();
		    interval_id = setInterval(letsGlow, 1500);
		} else {
		    if (typeof interval_id != "undefined") {
			clearInterval(interval_id);
		    }
		    glow.hide();
		}
	    }
	    jQuery('a#retrieveTitle').on("click", function() {
		var getURL = jQuery("input#websiteURL").val();
		var tastoRetrieve = jQuery(this);
		tastoRetrieve.children("i").removeClass().addClass("fas fa-spinner fa-spin");
		jQuery.ajax({
		    type: "POST",
		    url: "fetch-url",
		    data: {websiteURL: getURL}
		})
			.done(function(msg) {
			    var mess = msg.split("||");
			    if (mess[0] === "KO") {
				alert(mess[1]);
				jQuery("input#IEtiletitle").val("");
			    } else {
				jQuery("input#IEtiletitle").val(mess[1]);
			    }
			    tastoRetrieve.children("i").removeClass().addClass("fas fa-magic");
			});
	    });
	</script>
    </div>
</div>
